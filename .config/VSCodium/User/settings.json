{
    "editor.fontFamily": "'Iosevka Custom', 'Droid Sans Mono', 'monospace', monospace",
    "cSpell.language": "en-GB",
    "git.enableSmartCommit": true,
    "explorer.confirmDragAndDrop": false,
    "workbench.colorTheme": "Gruvbox Dark Medium",
    "git.confirmSync": false,
    "vscord.app.name": "VSCodium",
    "cSpell.userWords": [
        "Neplan",
        "NPCS",
        "WASD"
    ],
    "explorer.confirmDelete": false,
    "markyMarkdown.statsShowReadingTime": false,
    "markyMarkdown.statsShowWords": true,
    "markyMarkdown.statsShowLines": false,
    "markyMarkdown.statsShowCharacters": false,
    "tabnine.experimentalAutoImports": true,
    "grammarly.files.include": [
        "**/readme.md",
        "**/README.md",
        "**/*.txt",
        "**/*.md",
        "**/*.tex"
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "aws.telemetry": false,
    "grammarly.config.documentDomain": "academic",
    "grammarly.config.documentDialect": "australian"
}